  "scripts": {
    "dev:frontend": "cd frontend && vite",
    "dev:backend": "cd backend && tsx src/main.ts",
    "dev": "concurrently \"pnpm run dev:backend\" \"pnpm run dev:frontend\"",
    "build:frontend": "cd frontend && tsc -p tsconfig.json && vite build",
